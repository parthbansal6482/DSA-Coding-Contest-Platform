# Environment Variables
NODE_ENV=production
PORT=5001

# MongoDB
# For local: mongodb://localhost:27017/dsa-contest
# For Atlas: mongodb+srv://<username>:<password>@cluster.mongodb.net/dsa-contest
MONGODB_URI=

# JWT Secret
JWT_SECRET=provide-a-long-random-string-here
JWT_EXPIRE=7d

# CORS (Comma separated list of allowed origins)
ALLOWED_ORIGINS=https://your-frontend-domain.com,http://localhost:3000
